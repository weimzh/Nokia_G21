/*-SCT-702, fangduozhu.wt, modify, 20210916, battery bringup*/
			charger-manager {
				compatible = "charger-manager";
				cm-name = "battery";
				cm-poll-mode = <2>;
				cm-poll-interval = <15000>;
				cm-battery-stat = <2>;

				cm-fullbatt-vchkdrop-ms = <30000>;
				cm-fullbatt-vchkdrop-volt = <100000>;
/*+SCT-702, fangduozhu.wt, modify, 20210923, let status can be full*/
				cm-fullbatt-voltage = <4384000>;
				cm-fullbatt-current = <200000>;
/*-SCT-702, fangduozhu.wt, modify, 20210923, let status can be full*/
				cm-fullbatt-capacity = <100>;
				/*Charge pump target charge current*/
				/*cm-cp-taper-current = <1000000>;*/

				cm-num-chargers = <1>;
				cm-num-charge-pumps = <1>;
				cm-num-fast-chargers = <1>;
				/*cm-num-wireless-chargers = <1>;
				cm-num-wireless-charge-pump-converters = <1>;
				*/
				cm-chargers = "bq25890_charger";
				/*cm-charge-pumps = "bq2597x-standalone";*/
				cm-fast-chargers = "sc2730_fast_charger";/*modify by wt.liuzhiqing for SCT-702,open fastcharge sc2730 */
				/*cm-wireless-chargers = "nu1619_wireless_charger";
				cm-wireless-charge-pump-converters = "hl1506_cp_converter";
				*/
				cm-fuel-gauge = "sc27xx-fgu";

				/* in deci centigrade */
				cm-battery-cold = <000>;
				cm-battery-cold-in-minus;
				cm-battery-hot = <550>;
				cm-battery-temp-diff = <30>;

				/* Allow charging for 8hr */
				cm-charging-max = <43200000>;
				/* recovery charging after stop charging 45min */
				cm-discharging-max = <2700000>;

				/* the interval to feed charger watchdog */
				cm-wdt-interval = <60>;

				/* drop voltage in microVolts to allow shutdown */
				cm-shutdown-voltage = <3100000>;

				/* when 99% of the time is exceeded, it will be forced to 100% */
				cm-tickle-time-out = <1500>;

				/* how much time to allow capacity change */
				cm-one-cap-time = <30>;
				cm-thermal-zone = "board-thmzone";
				/* when the safe charging voltage is exceeded, stop charging *//*9V 2A*/
				cm-charge-voltage-max = <6500000>;
				cm-fast-charge-voltage-max = <10500000>;
				/*cm-flash-charge-voltage-max = <10000000>;*/
				/*cm-wireless-charge-voltage-max = <6500000>;
				cm-wireless-fast-charge-voltage-max = <12000000>;*/
				/* drop voltage in microVolts to restart charging */
				cm-charge-voltage-drop = <700000>;
				cm-fast-charge-voltage-drop = <700000>;
				/*cm-flash-charge-voltage-drop = <1000000>;*/
				/*cm-wireless-charge-voltage-drop = <700000>;
				cm-wireless-fast-charge-voltage-drop = <1000000>;*/

				cm-dcp-jeita-temp-table = <1000 1030 0 4384000>, <1150 1180 800000 4384000>,
					<1450 1420 2500000 4384000>, <1550 1520 800000 4100000>;
				cm-sdp-jeita-temp-table = <1000 1030 0 4384000>, <1150 1180 500000 4384000>,
					<1450 1420 500000 4384000>, <1550 1520 500000 4100000>;
				cm-cdp-jeita-temp-table = <1000 1030 0 4384000>, <1150 1180 800000 4384000>,
					<1450 1420 1500000 4384000>, <1550 1520 800000 4100000>;
				cm-unknown-jeita-temp-table = <1000 1030 0 4384000>, <1150 1180 500000 4384000>,
					<1450 1420 500000 4384000>, <1550 1520 500000 4100000>;
				cm-fchg-jeita-temp-table = <1000 1030 0 4384000>, <1150 1180 800000 4384000>,
					<1450 1420 3500000 4384000>, <1600 1570 800000 4100000>;
				/*cm-flash-jeita-temp-table = <1000 1030 0 4384000>, <1150 1180 1000000 4384000>,
					<1450 1420 5000000 4384000>, <1600 1570 1000000 4100000>;
				cm-wl-bpp-jeita-temp-table = <1000 1030 0 4384000>, <1150 1180 500000 4384000>,
					<1450 1420 1000000 4384000>, <1600 1570 5000000 4100000>;
				cm-wl-epp-jeita-temp-table = <1000 1030 0 4384000>, <1150 1180 500000 4384000>,
					<1450 1420 2000000 4384000>, <1600 1570 500000 4100000>;
				*/
				cm-cap-remap-table = <1 2 3>, <2 3 2>, <98 99 2>, <99 100 3>;

/*SCT-702, fangduozhu.wt, modify, 20210916, battery bringup*/
				monitored-battery = <&bat1 &bat2 &bat>;
				cm-capacity-track;

				cm-ir-cv-offset-microvolt = <100000>;
				cm-ir-rc = <20>;
				cm-ir-us-upper-limit-microvolt = <4500000>;

				regulator@0 {
					cm-regulator-name = "vddgen0";
					cable@0 {
						cm-cable-name = "USB";
						extcon = <&pmic_typec>;
					};
				};
			};
