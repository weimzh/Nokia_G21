					label = "Volume Down Key";
					linux,code = <KEY_VOLUMEDOWN>;
					gpios = <&ap_gpio 126 GPIO_ACTIVE_LOW>;/*Modify by wt.liuzhiqing for SCT-702, volumedown used gpio126*/
